{
  "name": "millegrilles.nodejs",
  "version": "2022.0.0",
  "description": "Utilitaires communs serveur NodeJS pour MilleGrilles",
  "author": "dugrema",
  "license": "MIT",
  "repository": "dugrema/millegrilles.nodejs",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@dugrema/millegrilles.utiljs": "https://github.com/dugrema/millegrilles.utiljs.git#2022.0.0",
    "@dugrema/node-forge": "github:dugrema/forge#ed25519",
    "amqplib": "^0.8.0",
    "axios": "^0.24.0",
    "base64url": "^3.0.1",
    "connect-redis": "^6.0.0",
    "crypto": "^1.0.1",
    "express-session": "^1.17.2",
    "express-socket.io-session": "^1.3.5",
    "fido2-lib": "^2.6.8",
    "morgan": "^1.10.0",
    "node-forge": "^1.0.0",
    "redis": "^3.1.2",
    "socket.io": "^4.4.0",
    "spdy": "^4.0.2",
    "uuid": "^8.3.2"
  }
}
